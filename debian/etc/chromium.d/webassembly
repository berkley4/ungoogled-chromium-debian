#export CHROMIUM_FLAGS="$CHROMIUM_FLAGS --js-flags=--noexpose_wasm"

# WebAssembly Dynamic Tiering
export CHROMIUM_FLAGS="$CHROMIUM_FLAGS --enable-blink-features=WebAssemblyDynamicTiering"
